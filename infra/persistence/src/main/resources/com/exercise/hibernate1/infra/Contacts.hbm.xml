<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.exercise.hibernate1.core.Contacts" table="Contacts">
      <id name="contactId" type="long" column="contactId">
         <generator class="increment"/>
      </id>
    <property name="contactType" column="contact_type" type="string"/>
	  <property name="contactValue" column="contact_value" type="string"/>
   </class>
</hibernate-mapping>
